<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Administrator.AdminDashboardController">
   <children>
      <Pane prefHeight="100.0" prefWidth="900.0" style="-fx-background-color: #42d7c1;">
         <children>
            <Label fx:id="nameLabel" layoutX="35.0" layoutY="22.0" prefHeight="58.0" prefWidth="550.0" text="Witaj na koncie administratora">
               <font>
                  <Font size="40.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button layoutX="796.0" layoutY="21.0" mnemonicParsing="false" onMouseClicked="#logout" text="Wyloguj się" />
      <Pane layoutY="100.0" prefHeight="600.0" prefWidth="150.0" style="-fx-background-color: #42d7c1;">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#visitList" prefHeight="40.0" prefWidth="150.0" text="Dodaj" />
            <Button layoutY="40.0" mnemonicParsing="false" onMouseClicked="#details" prefHeight="40.0" prefWidth="150.0" text="Szczegóły" />
            <Button layoutY="80.0" mnemonicParsing="false" onMouseClicked="#newBook" prefHeight="40.0" prefWidth="150.0" text="Nowa książka" />
            <Button layoutY="120.0" mnemonicParsing="false" onMouseClicked="#ksiazki" prefHeight="40.0" prefWidth="150.0" text="Księgarnia" />
            <Button layoutY="160.0" mnemonicParsing="false" onMouseClicked="#status" prefHeight="40.0" prefWidth="150.0" text="Statusy" />
            <Button layoutY="200.0" mnemonicParsing="false"  onMouseClicked="#raporty" prefHeight="40.0" prefWidth="150.0" text="Raporty" />
         </children>
      </Pane>
      <Pane fx:id="mainArea" layoutX="150.0" layoutY="100.0" prefHeight="600.0" prefWidth="750.0" >
      </Pane>
   </children>
</AnchorPane>